@model SistemaServicios.Models.Tecnico

@{
    ViewData["Title"] = "Detalles del Técnico";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>Técnico</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>Nombre</dt>
        <dd>@Model.Nombre</dd>
        <dt>Especialidad</dt>
        <dd>@Model.Especialidad</dd>
        <dt>Teléfono</dt>
        <dd>@Model.Telefono</dd>
    </dl>
</div>

<div>
    <a href="@Url.Action("Index")">Volver a la lista</a>
</div>
